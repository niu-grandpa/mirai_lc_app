    "adm-zip": "^0.5.15",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^7.0.0",
    "inserturlparams": "^1.0.1",
    "jest": "^29.7.0",
    "jet-logger": "^1.3.1",
    "jet-validator": "^1.1.1",
    "jsonfile": "^6.1.0",
    "jsonwebtoken": "^9.0.1",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "mysql2": "^3.11.0",
    "nanoid": "^3.3.7",
