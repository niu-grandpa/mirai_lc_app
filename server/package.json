    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/find": "^0.2.1",
    "@types/fluent-ffmpeg": "^2.1.21",
    "@types/fs-extra": "^11.0.1",
    "@types/jasmine": "^4.3.5",
    "@types/jsonfile": "^6.1.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.5.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
