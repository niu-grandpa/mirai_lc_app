{
  "name": "server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "test": "jest",
    "build": "npx ts-node build.ts",
    "start": "node -r module-alias/register ./dist --env=production",
    "dev": "nodemon",
    "server": "nodemon  --env=production"
  },
  "nodemonConfig": {
    "watch": [
      "src",
      "constants",
      "database"
    ],
    "ext": "ts, html",
    "ignore": [
      "public"
    ],
    "exec": "./node_modules/.bin/ts-node --files -r tsconfig-paths/register ./src"
  },
  "_moduleAliases": {
    "@src": "dist"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "dependencies": {
    "@jest/globals": "^29.7.0",
    "@types/adm-zip": "^0.5.5",
